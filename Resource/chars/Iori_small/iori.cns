;---<> OVER WRITE STAND <>---
;============================
; Stand
[Statedef 0]
type = S
physics = S
sprpriority = 0

[State 0, 1]
type = ChangeAnim
trigger1 = Anim != 0 && Anim != 5 && anim != 1 && anim != 3
trigger2 = Anim = 5 && AnimTime = 0
value = ifelse(var(2)= 1,1,0)

[State 0, 2]
type = VelSet
trigger1 = Time = 0
y = 0

[State 0, 3] ;Stop moving if low velocity or 4 ticks pass
type = VelSet
trigger1 = Abs(Vel x) < 2
trigger2 = Time = 4
x = 0

[State 0, 4] ;Are you dead?
type = ChangeState
trigger1 = !alive
value = 5050

;---------------------------------------------------------------------------
; Crouching
[Statedef 11]
type    = C
physics = C
anim = ifelse(var(2) = 1, 4,11)
sprpriority = 0
velset = 0,0

[State 11, 1] ;Change from turning animation
type = ChangeAnim
trigger1 = Anim = 6 && AnimTime = 0 
value = ifelse(var(2) = 1, 4,11)
